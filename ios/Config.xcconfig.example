// Configuration settings for ADHD Tracker iOS app
// Copy this file to 'Config.debug.xcconfig' and 'Config.release.xcconfig'
// Update with your actual values

// App Configuration
PRODUCT_BUNDLE_IDENTIFIER = com.example.adhdtracker
MARKETING_VERSION = 1.0.0
CURRENT_PROJECT_VERSION = 1
PRODUCT_NAME = ADHD Tracker
DEVELOPMENT_TEAM = YOUR_TEAM_ID

// API Configuration (if needed for future backend integration)
API_BASE_URL = api.example.com
API_KEY = your_api_key_here

// Firebase Configuration (if needed for future auth integration)
FIREBASE_API_KEY = your_firebase_api_key
FIREBASE_PROJECT_ID = your_firebase_project_id
FIREBASE_STORAGE_BUCKET = your_firebase_storage_bucket
FIREBASE_IOS_CLIENT_ID = your_firebase_ios_client_id
FIREBASE_BUNDLE_ID = $(PRODUCT_BUNDLE_IDENTIFIER)

// Feature Flags
ENABLE_ANALYTICS = YES
ENABLE_CRASH_REPORTING = YES

// Do not commit the actual Config.debug.xcconfig or Config.release.xcconfig files